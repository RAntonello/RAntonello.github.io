<div class="row" >
   <div class="small-12 columns" id="list_content_area">
	   
		   <h2>
			   Subscribe to a Mailing List
		   </h2>
		
		

		<!-- begin default_screen.tmpl -->






 
	<div class="panel">
		
			<div id="modal_subscribe_form">
		
			<!-- begin subscription_form_widget.tmpl --> 

<form action="http://itcs-conf.org/mail/dada/mail.cgi" method="post" accept-charset="UTF-8" id="subscription_form">
	<input type="hidden" name="flavor" id="ddm_flavor" value="subscribe" >

	    <div class="row">
	        <div class="small-12 columns">

		 
    
		     
		      <div class="row">
		        <div class="large-4 medium-4 columns">    		
			        <label 
						class="medium-text-right"
						for="ddm_list" 
					>
			       		Mailing List 
			        </label>
		        </div>
		        <div class="large-8 medium-8 columns">
					<select name="list" id="ddm_list">
<option value="announcements">ITCS Announcements</option>
</select>

				</div>
			</div>
		     
		


		  
			      <div class="row">
			        <div class="large-4 medium-4 columns">
						<label 
							class="medium-text-right"
					  		for="ddm_email" 
					  	>
							Email Address
						</label>
			        </div>
			        <div class="large-8 medium-8 columns">
						 
							<input 
								type="email" 
								name="email" 
								id="ddm_email" 
								value=""
								required="required" 
								placeholder="user@example.com (required)"
								>
						 
			        </div>
			      </div>

	  
		 
			<div id="ddm_subscriber_fields"> 
			 
			</div> 
		

		<div class="row">
			<div class="large-4 medium-4 columns">
				&nbsp;
			</div> 
			<div class="large-8 medium-8 columns">
						<input 
							type="submit" 
							value="Subscribe" 
							class="button success expand" 
							id="ddm_subscribe_button"
						>
			</div>
		</div>

		 

	</div>
	</div>
</form> 

<!-- end subscription_form_widget.tmpl --> 
 
		</div> 
	</div>

	

